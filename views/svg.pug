extends layout

block layout-content
  div.View
    h1.Banner HighChart Image Generate
    div.Container
      a.Item(href="undefinedchart.svg" target="_blank")
        img.Image(src="undefinedchart.svg")
        span.Title SVG
    div.NavButtons
      a(href="/")
        div.NavButton HOME
      a(href="/png-report")
        div.NavButton PNG Format
      a(href="/jpg-report")
        div.NavButton JPEG Format
      a.Disabled(href="/svg-report")
        div.NavButton SVG Format
      a(href="/base64-report")
        div.NavButton BASE64 Format