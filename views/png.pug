extends layout

block layout-content
  div.View
    h1.Banner HighChart Image Generate
    div.Container
      a.Item(href="/public/bar.png" target="_blank")
        img.Image(src="/public/bar.png")
        span.Title PNG
    div.NavButtons
      a(href="/")
        div.NavButton HOME
      a.Disabled(href="/png-report")
        div.NavButton PNG Format
      a(href="/jpg-report")
        div.NavButton JPEG Format
      a(href="/svg-report")
        div.NavButton SVG Format
      a(href="/base64-report")
        div.NavButton BASE64 Format